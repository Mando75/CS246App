<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.group4.readingapp.scripturereadingapp.EnableNotifications"
    tools:showIn="@layout/activity_enable_notifications">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/enable_notifi"
        android:onClick="enableNotify"
        tools:layout_editor_absoluteY="214dp"
        tools:layout_editor_absoluteX="101dp">
    </Button>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/disable_notifi"
        android:onClick="disableNotify"
        tools:layout_editor_absoluteY="298dp"
        tools:layout_editor_absoluteX="99dp" />

</android.support.constraint.ConstraintLayout>
